// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./users.db"
}


model Events {
  id Int @id @default(autoincrement())
  title String
  description String
  start DateTime
  end DateTime
  shiftStart DateTime
  shiftEnd DateTime
  allDay Boolean
}
